snippet e "begin ... end" b
\begin{$1}
	${0:${VISUAL}}
\end{$1}
endsnippet

snippet f "fraction" w
\frac{$1}{$2}
endsnippet

snippet m "inline math" w
\( ${1:${VISUAL}} \)
endsnippet

snippet M "display math" w
\[ ${1:${VISUAL}} \]
endsnippet

snippet ( "matching parentheses" w
\left( ${1:${VISUAL}} \right)
endsnippet

snippet [ "matching square brackets" w
\left[ ${1:${VISUAL}} \right]
endsnippet

snippet { "matching curly braces" w
\left\{ ${1:${VISUAL}} \right\}
endsnippet
